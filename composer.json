{
    "name": "santwer/exporter",
    "description": "Laravel Package to Export with tokens into word templates",
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.0|^9.0|^10|^11",
        "php": "^7.4|^8.0|^8.2|^8.2",
        "phpoffice/phpword": "^v1.2"
    },
    "autoload": {
        "psr-4": {
            "Santwer\\Exporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sven Antwertwetiner",
            "email": "sven.antwertinger@tricept.de"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Santwer\\Exporter\\ExporterProvider"
            ],
            "aliases": {
                "WordExport": "Santwer\\Exporter\\Facades\\WordExport"
            }
        }
    }
}
